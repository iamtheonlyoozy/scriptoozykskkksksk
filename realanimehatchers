local allowedPlaceID = 13264238461 

local currentPlaceID = tonumber(string.match(game.PlaceId, "%d+"))

if currentPlaceID ~= allowedPlaceID then
    game:GetService("Players").LocalPlayer:Kick("You are not allowed to run this script in this game.")
else
    
    loadstring(game:HttpGet("https://raw.githubusercontent.com/iamtheonlyoozy/scriptoozykskkksksk/main/AnimeHatchers"))()
end
